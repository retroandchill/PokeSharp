<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <TargetFramework>net9.0</TargetFramework>
    <InterceptorsNamespaces>$(InterceptorNamespaces);PokeSharp.Editor.Generated</InterceptorsNamespaces>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="$(PokeSharpProj)" />
    <ProjectReference Include="$(PokeSharpEditorCoreProj)" />
    <ProjectReference
      Include="$(PokeSharpEditorGeneratorProj)"
      OutputItemType="Analyzer"
      ReferenceOutputAssembly="false"
    />
  </ItemGroup>
</Project>
